{
  "version": 2,
  "name": "makebuild.dev",
  "alias": ["preview.makebuild.dev"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    },
    {
      "src": "api/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
